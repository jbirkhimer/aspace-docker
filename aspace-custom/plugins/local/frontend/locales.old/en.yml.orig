      string_1: Horizon number 
      string_1_tooltip: |
          <p>TBD: Associated Horizon bib number : siris_arc_999999. TESTING ONLY - DO NOT USE</p>
      text_1: Text 1 - testing
      text_1_tooltip: | 
        <p>FOR MIGRATED RECORDS ONLY.</p>
      date_1_tooltip: |
          <p>FOR MIGRATED RECORDS ONLY.</p>
      date_2_tooltip: |
          <p>FOR MIGRATED RECORDS ONLY.</p>
      date_3_tooltip: |
          <p>FOR MIGRATED RECORDS ONLY.</p>
      enum_1: New or Addition?
      enum_1_inline_help: FOR TESTING ONLY
      

